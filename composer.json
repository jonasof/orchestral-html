{
	"name": "orchestra/html",
	"description": "Orchestra Platform 2 Html Component",
	"homepage": "http://orchestraplatform.com/docs/latest/components/html/",
	"keywords": ["orchestra-platform", "orchestral", "laravel"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Html\\" : "src/Html/"
		}
	},
	"require": {
		"php": ">=5.3.3",
		"orchestra/support": "2.1.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"illuminate/config": "4.1.*",
		"illuminate/container": "4.1.*",
		"illuminate/database": "4.1.*",
		"illuminate/html": "4.1.*",
        "illuminate/pagination": "4.1.*",
		"illuminate/routing": "4.1.*",
		"illuminate/translation": "4.1.*",
		"illuminate/view": "4.1.*",
		"patchwork/utf8": "1.1.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/html component outside of Orchestra Platform 2",
		"orchestra/foundation": "Allow using orchestra/html with Orchestra Platform 2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.2-dev"
		}
	},
	"minimum-stability": "dev"
}
